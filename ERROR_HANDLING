def divide_numbers(a, b):
    try:
        result = a / b
    except ZeroDivisionError:
        return "Error: Cannot divide by zero!"
    else:
        return f"Result is {result}"

# Example usage
num1 = 10
num2 = 0

print(divide_numbers(num1, num2))  # This will catch division by zero

num2 = 2
print(divide_numbers(num1, num2))  # This will perform division successfully
